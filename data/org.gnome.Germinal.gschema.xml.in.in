<schemalist>
    <schema id="org.gnome.Germinal" path="/org/gnome/Germinal/" gettext-domain="@GETTEXT_PACKAGE@">

    <key name="scrollback-lines" type="i">
      <default>65536</default>
      <_summary>Number of lines to keep in the buffer</_summary>
      <_description>
        Number of lines to keep in the buffer
      </_description>
    </key>

    <key name="word-char-exceptions" type="s">
      <default>'-#%&amp;+,./;=?@\\_~\302\267'</default>
      <_summary>List of ASCII punctuation characters that should be considered as part of a word when doing word-wise selection</_summary>
      <_description>
        List of ASCII punctuation characters that should be considered as part of a word when doing word-wise selection
      </_description>
    </key>

    <key name="font" type="s">
      <default>'DejaVu Sans Mono 8.5'</default>
      <_summary>The font to use</_summary>
      <_description>
        The font to use
      </_description>
    </key>

    <key name="forecolor" type="s">
      <default>'#C64B9A'</default>
      <_summary>The foreground color</_summary>
      <_description>
        The foreground color
      </_description>
    </key>

    <key name="backcolor" type="s">
      <default>'#212121'</default>
      <_summary>The background color</_summary>
      <_description>
        The background color
      </_description>
    </key>

    <key name="palette" type="as">
      <!-- Stolen from sakura which stole it from gnome-terminal -->
      <default>
        [
          '#000000000000',
          '#cdcb00000000',
          '#0000cdcb0000',
          '#cdcbcdcb0000',
          '#1e1a908fffff',
          '#cdcb0000cdcb',
          '#0000cdcbcdcb',
          '#e5e2e5e2e5e2',
          '#4ccc4ccc4ccc',
          '#ffff00000000',
          '#0000ffff0000',
          '#ffffffff0000',
          '#46458281b4ae',
          '#ffff0000ffff',
          '#0000ffffffff',
          '#ffffffffffff'
        ]
      </default>
      <_summary>The color palette</_summary>
    </key>

    <key name="startup-command" type="s">
      <default>'tmux -u2 a'</default>
      <_summary>Command to launch at startup</_summary>
      <_description>
        If none is specified, tmux will be used
      </_description>
    </key>

    <key name="term" type="s">
      <default>'xterm-256color'</default>
      <_summary>The value of the TERM env variable</_summary>
      <_description>
        By default, vte uses xterm, we use xterm-256color
      </_description>
    </key>

  </schema>
</schemalist>
